{
    "namespace": "Ext",
    "name": "theme-material",
    "type": "theme",
    "extend": "theme-neptune",
    "toolkit": "modern",
    "creator": "Sencha",
    "summary": "Ext Js Modern Material Design Theme",
    "detailedDescription": "Material Design theme for modern devices",
    "version": "6.2.0.526",
    "compatVersion": "6.1.0",
    "format": "1",
    "output": "${workspace.subpkg.prefix}/${package.name}",
