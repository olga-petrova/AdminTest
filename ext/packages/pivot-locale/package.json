{
  "name": "pivot-locale",
  "format": "1",
  "version": "6.0.2.275",
  "compatVersion": "6.0.0",
  "creator": "Sencha",
  "summary": "Sencha Pivot locale",
  "detailedDescription": "Localization for the pivot package",
  "type": "code",
  "framework": "ext",
  "toolkit": "classic",
  "output": "${package.dir}/build",
  "sass": {
    "namespace": "Ext",
    "etc": "${package.dir}/sass/etc/all.scss",
    "var": "${package.dir}/sass/var",
    "src": "${package.dir}/sass/src"
  },
  "classpath": "${package.dir}/src",
  "overrides": "${package.dir}/overrides/${package.locale}"
}