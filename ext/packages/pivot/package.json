{
  "name": "pivot",
  "namespace": "Ext",
  "format": "1",
  "version": "6.0.2.275",
  "compatVersion": "6.0.0",
  "creator": "Sencha",
  "summary": "Sencha Pivot",
  "detailedDescription": "Pivot Grid component",
  "type": "code",
  "requires": [
    "exporter",
    "ux"
  ],
  "resources": [
    {
      "path": "resources"
    },
    {
      "path": "${toolkit.name}/resources"
    }
  ],
  "slicer": {
    "js": [
      {
        "path": "${package.dir}/sass/example/custom.js",
        "isWidgetManifest": true
      }
    ]
  },
  "builds": {
    "neptune": {
      "framework": "ext",
      "theme": "ext-theme-neptune"
    },
    "crisp": {
      "framework": "ext",
      "theme": "ext-theme-crisp"
    },
    "classic": {
      "framework": "ext",
      "theme": "ext-theme-classic"
    }
  },
  "framework": "ext",
  "toolkit": "classic",
  "output": {
    "base": "${package.dir}/build/${build.id}",
    "js": "..",
    "sass": ""
  },
  "sass": {
    "namespace": "Ext",
    "etc": "${package.dir}/sass/etc/all.scss",
    "var": "${package.dir}/sass/var",
    "src": "${package.dir}/sass/src"
  },
  "classpath": "${package.dir}/src",
  "overrides": "${package.dir}/overrides"
}